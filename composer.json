{
    "name": "drupal-composer/drupal-project",
    "description": "Project template for Drupal 8 projects with composer",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "",
            "role": ""
        }
    ],
    "repositories": {
        "drupal": {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        "assets": {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    },
    "require": {
        "php": ">=7.0.8",
        "bower-asset/dropzonejs": "^4.3",
        "composer/installers": "^1.2",
        "cweagans/composer-patches": "^1.6.5",
        "drupal-composer/drupal-scaffold": "^2.5",
        "drupal/admin_toolbar": "^2.0",
        "drupal/allowed_formats": "^1.1",
        "drupal/auto_entitylabel": "^3.0@beta",
        "drupal/better_exposed_filters": "^4.0@alpha",
        "drupal/config_update": "^1.6",
        "drupal/console": "^1.0.2",
        "drupal/context": "^4.0@beta",
        "drupal/core": "^8.7.0",
        "drupal/crop": "^1.5",
        "drupal/ctools": "^3.2",
        "drupal/date_popup": "^1.0",
        "drupal/devel": "^2.1",
        "drupal/diff": "^1.0@RC",
        "drupal/editor_advanced_link": "^1.4",
        "drupal/embed": "^1.0",
        "drupal/entity_browser": "^2.2",
        "drupal/entity_embed": "^1.0",
        "drupal/entity_reference_revisions": "^1.7",
        "drupal/environment_indicator": "^3.7",
        "drupal/eu_cookie_compliance": "^1.8",
        "drupal/features": "^3.8",
        "drupal/field_group": "^1.0",
        "drupal/focal_point": "^1.2",
        "drupal/fontawesome": "^2.14",
        "drupal/group": "^1.0@RC",
        "drupal/honeypot": "^1.29",
        "drupal/image_url_formatter": "^1.0@beta",
        "drupal/inline_entity_form": "^1.0@RC",
        "drupal/link_attributes": "^1.9",
        "drupal/linkit": "^4.3",
        "drupal/masquerade": "^2.0@beta",
        "drupal/media_entity_generic": "^1.0",
        "drupal/media_entity_instagram": "^2.0",
        "drupal/media_entity_twitter": "^2.1",
        "drupal/media_responsive_thumbnail": "^1.0",
        "drupal/menu_block": "^1.5",
        "drupal/metatag": "^1.10",
        "drupal/module_filter": "^3.1",
        "drupal/paragraphs": "^1.10",
        "drupal/pathauto": "^1.5",
        "drupal/rabbit_hole": "^1.0@beta",
        "drupal/redirect": "^1.4",
        "drupal/search_exclude": "^1.2",
        "drupal/shield": "^1.2",
        "drupal/simple_sitemap": "^3.4",
        "drupal/smart_trim": "^1.1",
        "drupal/token": "^1.5",
        "drupal/twig_field_value": "^1.2",
        "drupal/video_embed_field": "^2.2",
        "drupal/view_unpublished": "^1.0@alpha",
        "drupal/webform": "^5.5",
        "oomphinc/composer-installers-extender": "^1.1",
        "vlucas/phpdotenv": "^2.4",
        "webflo/drupal-finder": "^1.0.0",
        "webmozart/path-util": "^2.3",
        "zaporylie/composer-drupal-optimizations": "^1.0"
    },
    "require-dev": {
        "webflo/drupal-core-require-dev": "^8.7.0"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ],
        "files": ["load.environment.php"]
    },
    "scripts": {
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ]
    },
    "extra": {
        "composer-exit-on-patch-failure": true,
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "installer-paths": {
            "web/core": ["type:drupal-core"],
            "web/libraries/{$name}": 
            [
                "type:drupal-library",
                "type:bower-asset",
                "type:npm-asset"
            ],
            "web/modules/contrib/{$name}": ["type:drupal-module"],
            "web/profiles/contrib/{$name}": ["type:drupal-profile"],
            "web/themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/Commands/contrib/{$name}": ["type:drupal-drush"]
        },
        "drupal-scaffold": {
            "initial": {
                ".editorconfig": "../.editorconfig",
                ".gitattributes": "../.gitattributes"
            }
        }
    }
}
